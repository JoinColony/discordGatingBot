{
  "name": "discord-gating-bot-frontend",
  "version": "1.0.0",
  "description": "The frontend for the infamouse discord-gating-bot",
  "main": "dist/index.ts",
  "scripts": {
    "serve": "esbuild --bundle --outdir=www --servedir=www src/index.ts",
    "build": "esbuild --bundle src/index.ts --minify --outfile=dist/index.js && cpy --flat www/index.html dist"
  },
  "keywords": [
    "colony",
    "blockchain",
    "web3"
  ],
  "author": "Christian Maniewski <chris@colony.io>",
  "license": "GPL-3.0-only",
  "devDependencies": {
    "cpy-cli": "^4.1.0",
    "esbuild": "^0.17.8",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "ethers": "^5.7.2"
  }
}
